{
    "version": "0.2.0",
    "configurations": [
        
        {
            "name": "ROS: Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/install/lio_livox_ros2/lib/lio_livox_ros2/scan_registration",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [
                {
                    "name": "AMENT_PREFIX_PATH",
                    "value": "${workspaceFolder}/install:${env:AMENT_PREFIX_PATH}"
                }
            ],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "gdbの印刷を見やすくする",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "build"
        }
    ]
}